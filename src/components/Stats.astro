<script>
  const cards = document.querySelectorAll("#stat");

  const options = {
    root: null,
    rootMargin: "0px",
    threshold: 0.5,
  };

  const handleIntersect = (entries, observer) => {
    entries.forEach((entry) => {
      if (entry.isIntersecting) {
        entry.target.classList.add("animate__flipInX");
        observer.unobserve(entry.target);
      }
    });
  };

  const obserberImg = new IntersectionObserver(handleIntersect, options);

  cards.forEach((card) => {
    obserberImg.observe(card);
  });
</script>
